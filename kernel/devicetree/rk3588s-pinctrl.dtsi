		/omit-if-no-ref/
		uart2m0_xfer: uart2m0-xfer {
			rockchip,pins =
				/* uart2_rx_m0 */
				<0 RK_PD1 1 &pcfg_pull_up>,
				/* uart2_tx_m0 */
				<0 RK_PD0 1 &pcfg_pull_up>;
		};

		/omit-if-no-ref/
		uart2m1_xfer: uart2m1-xfer {
			rockchip,pins =
				/* uart2_rx_m1 */
				<4 RK_PA6 3 &pcfg_pull_up>,
				/* uart2_tx_m1 */
