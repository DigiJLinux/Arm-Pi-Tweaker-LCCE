			rockchip,pins =
				/* spi0_mosi */
				<2 RK_PC7 2 &pcfg_pull_up>;
		};
	};

	spi1 {
		/omit-if-no-ref/
		spi1m0_clk: spi1m0-clk {
			rockchip,pins =
				/* spi1_clk_m0 */
				<1 RK_PB0 3 &pcfg_pull_up>;
		};

		/omit-if-no-ref/
		spi1m0_cs0: spi1m0-cs0 {
			rockchip,pins =
