				/* i2c8_sda */
				<4 RK_PD3 1 &pcfg_pull_none_drv_level_5>;
		};
	};

	pmic {
		/omit-if-no-ref/
		pmic_pins: pmic-pins {
			rockchip,pins =
				/* pmic_int_l */
				<0 RK_PA7 0 &pcfg_pull_up>;
		};
	};

	pwm0 {
		/omit-if-no-ref/
		pwm0_pin: pwm0-pin {
			rockchip,pins =
				/* pwm0_out */
				<0 RK_PC7 1 &pcfg_pull_none>;
