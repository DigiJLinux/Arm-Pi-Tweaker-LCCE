		/omit-if-no-ref/
		uart1m0_xfer: uart1m0-xfer {
			rockchip,pins =
				/* uart1_rx_m0 */
				<2 RK_PB7 2 &pcfg_pull_up>,
				/* uart1_tx_m0 */
				<2 RK_PB6 2 &pcfg_pull_up>;
